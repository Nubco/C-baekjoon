#include<stdio.h>

int main(){
    int n;
    scanf("%d",&n);
    int a[n];
    for (int i = 0; i < n; i++) {
		scanf("%d ", &a[i]);
	}
    int b;
    scanf("%d",&b);
    
    int c=0;
    for(int i=0;i<n;i++){
        if(b==a[i]){
            c=c+1;
        }
        
    }
    printf("%d",c);
    return 0;
}
